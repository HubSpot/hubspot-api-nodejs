{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "sourceMap": true,
    "outDir": "./lib",
    "noLib": false,
    "lib": [
      "es6"
      ,"ES2017.Object"
      ,"ES2021.String"
    ],
  },
  "exclude": [
    "lib",
    "node_modules"
  ]
}
